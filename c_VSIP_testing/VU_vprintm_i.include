#ifndef VU_VPRINTM_I_H
#define VU_VPRINTM_I_H 1

static void VU_vprintm_i(char s[], vsip_vview_i *X) {
    char format[50];
    vsip_length L = vsip_vgetlength_i(X);
    vsip_index vi;
    vsip_scalar_i x;
    strcpy(format,"%");
    strcat(format,s);
    strcat(format,"d %s\n");
    printf("[\n");
    for(vi=0; vi<L; vi++){
      x=vsip_vget_i(X,vi);
      printf(format,x,";");
    }
    printf("];\n");
    return;
}

#endif
