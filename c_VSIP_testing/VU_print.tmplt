static void VU_vprintm_bl( vsip_vview_bl *X) {
    vsip_length L = vsip_vgetlength_bl(X);
    vsip_index vi;
    vsip_scalar_bl x;
    printf("[\n");	
    for(vi=0; vi<L; vi++){
      x=vsip_vget_bl(X,vi);
      x = (x == VSIP_FALSE) ? 0 : 1;
      printf("%1d %s",x,";");
    }  
    printf("];\n");
    return;
}

