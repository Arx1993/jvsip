#ifndef VU_VPRINTM_SI_H
#define VU_VPRINTM_SI_H 1

static void VU_vprintm_si(char s[], vsip_vview_si *X) {
    char format[50];
    vsip_length L = vsip_vgetlength_si(X);
    vsip_index vi;
    vsip_scalar_si x;
    strcpy(format,"%");
    strcat(format,s);
    strcat(format,"d %s\n");
    printf("[\n");
    for(vi=0; vi<L; vi++){
      x=vsip_vget_si(X,vi);
      printf(format,x,";");
    }
    printf("];\n");
    return;
}
#endif
